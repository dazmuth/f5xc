\pard\tx220\tx720\pardeftab720\li720\fi-720\sl480\sa500\partightenfactor0
\ls1\ilvl0
\b0 \cf2 \kerning1\expnd0\expndtw0 \outl0\strokewidth0 		\expnd0\expndtw0\kerning0
\outl0\strokewidth0 \strokec2 In upper-left corner click 
\b Select service
\b0 , then click 
\b Multi-Cloud Network Connect
\b0 \cb1 \
\ls1\ilvl0\cb3 \kerning1\expnd0\expndtw0 \outl0\strokewidth0 		\expnd0\expndtw0\kerning0
